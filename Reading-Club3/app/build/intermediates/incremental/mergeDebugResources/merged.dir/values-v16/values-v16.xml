<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="notification_right_side_padding_top">4dp</dimen>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FF807663</item>
        <item name="colorPrimaryDark">#FF807663</item>
        <item name="colorAccent">#FF807663</item>
        <item name="android:fontFamily">@font/ralewaybold</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="TextAppearance.AppCompat.Tooltip">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>